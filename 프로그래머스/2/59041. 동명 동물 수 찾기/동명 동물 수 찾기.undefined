# SELECT
#       NAME,
#       COUNT(NAME) AS COUNT
#   FROM ANIMAL_INS
#  WHERE NAME IN (
# SELECT 
#        NAME
#   FROM ANIMAL_INS
#  GROUP BY NAME
#  HAVING COUNT(NAME) >= 2
# )

SELECT 
       NAME, 
       COUNT(NAME) AS COUNT
  FROM ANIMAL_INS 
 GROUP BY NAME 
 HAVING COUNT(NAME) >= 2
 ORDER BY NAME ASC;